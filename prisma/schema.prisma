// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  // Connection URL to your database 
  url       = env("DATABASE_URL")
  // Direct connection URL to the database used for migrations
  // https://www.prisma.io/docs/guides/performance-and-optimization/connection-management#external-connection-poolers
  directUrl = env("DIRECT_URL")
}

model MeduzaArticles {
  id             Int       @id @default(autoincrement())
  header         String?
  subtitle       String?
  date           DateTime?
  dateString     String?
  currentLink    String?
  nextLink       String?
  photosWithMeta Json
  createdAt      DateTime? @default(now())
  updatedAt      DateTime? @default(now())
}
